{
	"multipart": [
		{
			"apply": {
				"model": "$MOD:block/$BLOCK_post"
			}
		},
		{
			"when": {
				"north": "true"
			},
			"apply": {
				"model": "$MOD:block/$BLOCK_side"
			}
		},
		{
			"when": {
				"east": "true"
			},
			"apply": {
				"model": "$MOD:block/$BLOCK_side",
				"y": 90
			}
		},
		{
			"when": {
				"south": "true"
			},
			"apply": {
				"model": "$MOD:block/$BLOCK_side_alt"
			}
		},
		{
			"when": {
				"west": "true"
			},
			"apply": {
				"model": "$MOD:block/$BLOCK_side_alt",
				"y": 90
			}
		},
		{
			"when": {
				"north": "false"
			},
			"apply": {
				"model": "$MOD:block/$BLOCK_noside"
			}
		},
		{
			"when": {
				"east": "false"
			},
			"apply": {
				"model": "$MOD:block/$BLOCK_noside_alt"
			}
		},
		{
			"when": {
				"south": "false"
			},
			"apply": {
				"model": "$MOD:block/$BLOCK_noside_alt",
				"y": 90
			}
		},
		{
			"when": {
				"west": "false"
			},
			"apply": {
				"model": "$MOD:block/$BLOCK_noside",
				"y": 270
			}
		}
	]
}